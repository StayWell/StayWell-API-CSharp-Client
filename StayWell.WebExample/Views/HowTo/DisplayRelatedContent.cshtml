@using StayWell.ServiceDefinitions.Content.Objects
@using StayWell.WebExample.Models

@model RelatedContentModel
@{
    ViewBag.Title = "Display Related Content";
}
<div class="section-header">
    <h1>How To: Get Related Content</h1>
</div>
<h3>Content Related To: @Model.OriginalArticleTitle</h3>
<ul>
    @foreach (ContentResponse item in Model.RelatedContent)
    {
        <li>@item.Title</li>
    }
</ul>

